    <!-- Page Content -->
<main role="main" class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h1 class="mt-5">Administration du CF2m</h1>
                <p class="lead">Gestion des sessions</p>               
            </div>
            <div <div class="col-lg-12 text-center">
            <h4><a href="?insertlasession={{addsession.idlasession}}">Ajouter une session</a></h4> 
            </div>
            <br>
            <div class="col-lg-6 text-left">
                {# Faites apparaître les sections (celles du menu) cliquables dans un nouveau h4, sinon écrivez "Pas encore d'éléments dans le menu" #}
                {% for item in detailsession %}
                    <h4 class="mt-3">{{ item.lenom }} | <a href="?updatelasession={{ item.idlasession }}">modifier</a> | <a href="?deletelasession={{ item.idlasession }}">supprimer</a></h4>
                    <p>{{ item.lacronyme }}</p>
					<p>{{ item.lannee }}</p>
					<p>{{ item.lenumero }}</p>
					<p>{{ item.letype }}</p>
					<p>{{ item.debut }}</p>
					<p>{{ item.fin }}</p>
                {% else %}
                <h4 class="mt-3">Pas encore de session sur notre page accueil</h4>
                {% endfor %}
            </div>
        </div>
</main>